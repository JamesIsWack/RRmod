*/
//-----------------------------------------------------------------------------------------------

//Default texture to apply to object
TEXTURE LightmapTexture
<
	STRING ResourceName = "Lightmap_lm.dds";
	STRING ResourceType = "2D";
>;

//Default 2D sampler to apply to object
SAMPLER2D LightmapSampler = sampler_state
{
	Texture = <LightmapTexture>;
  	MagFilter = Linear;
  	MinFilter = LinearMipMapLinear;
	AddressU = WRAP;
	AddressV = WRAP;	
};

FLOAT3 GetLightmap(FLOAT2 texCoord)
{
	return tex2D(LightmapSampler, texCoord.xy).rgb;
}

void LightMapInfluence(FLOAT3 normal, FLOAT3 worldNormal, FLOAT3 worldEyeVec, FLOAT2 diffuseUV, FLOAT2 lightMapUV,
						 FLOAT3 inDiffuse, FLOAT3 inSpecular, FLOAT3 light,
						 out FLOAT3 diffuseLM, out FLOAT3 specLM)
{ 
    FLOAT4 lighting = lit(1.0f,dot(normalize(worldEyeVec + normal),normal),SpecExpon);
    
	FLOAT3 lmapColor = GetLightmap(lightMapUV) * CalcDiffuse(lighting.y, light, diffuseUV);
    FLOAT3 lmapSpec = 0.0;
    FLOAT3 lmapDiff = lmapColor*light;

    diffuseLM = inDiffuse;
	specLM = inSpecular;
	
    if (lighting.y > 0.0)
	{
		FLOAT nx = dot(normal, worldNormal);
		FLOAT specExpon = 1.0f - pow(nx,16);

		lmapSpec = CalcSpecular(lighting.z, lmapDiff) * Ks * specExpon;
	}

	diffuseLM += lmapDiff;
	specLM += lmapSpec;
}


