//Default Normal Map Texture
TEXTURE CubeTexture
<
    STRING ResourceName = "Drummer_d.dds";
    STRING ResourceType = "CUBE";
>;

//Default Normal Map sampler
SAMPLERCUBE CubeSampler = sampler_state
{
	Texture = <CubeTexture>;
  	MinFilter = LinearMipMapLinear;
	MagFilter = Linear;
};

FLOAT Reflectivity : CUBEREFLECTIVITY
<
	STRING UIName = "Cube Map Reflectivity";
	STRING UIWidget = "slider";
	FLOAT UIMin = 0.0;
	FLOAT UIMax = 8;
	FLOAT UIStep = 0.05;
> = 1;

FLOAT3 CalculateReflectionVector (FLOAT3 position, FLOAT3 normal, FLOAT3 eyePositionEnv)
{
    FLOAT3 eyeVector = position - eyePositionEnv;
    return normalize(reflect(eyeVector, normal));
}

FLOAT3 GetCubeMap(FLOAT3 reflectionVector)
{
	return (FLOAT3)texCUBE( CubeSampler, reflectionVector ) * Reflectivity;
}

